IDR_FROTZ MENU 
BEGIN
    POPUP "&File"
    BEGIN
        MENUITEM "&New Game...\tCtrl+N",        ID_FILE_NEW
        MENUITEM "&Open Saved Game...\tCtrl+O", ID_FILE_OPEN
        MENUITEM "&Save Game...\tCtrl+S",       ID_FILE_SAVE
        MENUITEM "S&top Playback\tCtrl+T",      ID_FILE_STOP
        MENUITEM SEPARATOR
        MENUITEM "E&xit\tAlt+F4",               ID_APP_EXIT
    END
    POPUP "&View"
    BEGIN
        MENUITEM "&Toolbar",                    ID_VIEW_TOOLBAR
        MENUITEM "&Status Bar",                 ID_VIEW_STATUS_BAR
        MENUITEM SEPARATOR
        MENUITEM "O&ptions...\tCtrl+P",         ID_VIEW_OPTIONS
        MENUITEM "Scro&llback\tCtrl+L",         ID_VIEW_SCROLLBACK
    END
    POPUP "&Help"
    BEGIN
        MENUITEM "Windows Frotz &Help",         ID_HELP_FINDER
        POPUP "Internet Links"
        BEGIN
            MENUITEM "Windows Frotz",           ID_LINKS_FROTZ
            MENUITEM SEPARATOR
            MENUITEM "The Interactive Fiction Archive", ID_LINKS_ARCHIVE
            MENUITEM "Z-code Games at the IF Archive", ID_LINKS_ZCODE
            MENUITEM "Interactive Fiction Database", ID_LINKS_IFDB
            MENUITEM SEPARATOR
            MENUITEM "intfiction.org Forum",        ID_LINKS_INTFICTION
            MENUITEM "Planet IF",                   ID_LINKS_PLANETIF
            MENUITEM "IF Wiki",                     ID_LINKS_IFWIKI
            MENUITEM "IF Competition",              ID_LINKS_IFCOMP
            MENUITEM "ifMUD",                       ID_LINKS_IFMUD
            MENUITEM SEPARATOR
            MENUITEM "Inform 7",                    ID_LINKS_INFORM7
            MENUITEM "Inform 6",                    ID_LINKS_INFORM6
        END
        MENUITEM SEPARATOR
        MENUITEM "About This &Game",            ID_APP_ABOUT_GAME
        MENUITEM "&About Windows Frotz",        ID_APP_ABOUT
    END
END


