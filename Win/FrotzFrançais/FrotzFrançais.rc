            MENUITEM "Inform 6",                    ID_LINKS_INFORM6
        END
        MENUITEM SEPARATOR
        MENUITEM "A propos de ce je&u",         ID_APP_ABOUT_GAME
        MENUITEM "&A propos de Windows Frotz",  ID_APP_ABOUT
    END
END


